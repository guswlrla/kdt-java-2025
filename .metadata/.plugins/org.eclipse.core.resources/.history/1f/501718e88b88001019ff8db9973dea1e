package com.ruby.java.ch03_if;

public class 실습_3_1주석문 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 자주 쓰는 코드 template 단축어: main, sysout, for,while, do
		 * 
		 * 코드 자동 완성: ctrl + space키
		 */
		System.out.println();
		int sum = 0;
		for (int i = 0; i < 10; i++) {
			sum += i;
		}
	}

}
